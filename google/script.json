{
  "info": {
    "name": "Google",
    "description": "Creates a Gmail account with associated mail address"
  },
  "create": {
    "headless": false,
    "options": {
      "checkboxes": [],
      "radioboxes": [],
      "textfields": [
        {
          "variable": "WA",
          "regex": ".*@gmail.com"
        },
        {
          "variable":"PW",
          "regex":".*"
        }
      ]
    },
    "actions": [
      {
        "type": "open",
        "url": "https://accounts.google.com/signup/v2/webcreateaccount?continue=https%3A%2F%2Fwww.google.com%2F&hl=de&dsh=S-1627029711%3A1595605022997153&gmb=exp&biz=false&flowName=GlifWebSignIn&flowEntry=SignUp"
      },
      {
        "type": "click",
        "element": "//*[@id=\"firstName\"]"
      },
      {
        "type": "write",
        "element": "//*[@id=\"firstName\"]",
        "value": "$FNAME"
      },
      {
        "type": "click",
        "element": "//*[@id=\"lastName\"]"
      },
      {
        "type": "write",
        "element": "//*[@id=\"lastName\"]",
        "value": "$LNAME"
      },
      {
        "type": "click",
        "element": "//*[@id=\"firstName\"]"
      },
      {
        "type": "write",
        "element": "//*[@id=\"firstName\"]",
        "value": "$FNAME"
      },
      {
        "type": "click",
        "element": "//*[@id=\"username\"]"
      },
      {
        "type": "write",
        "element": "//*[@id=\"username\"]",
        "value": "$WA"
      },
      {
        "type": "click",
        "element": "/html/body/div[1]/div/div[2]/div[1]/div[2]/form/div[2]/div/div[1]/div[3]/div[1]/div[1]/div/div/div[1]/div/div[1]/input"
      },
      {
        "type": "write",
        "element": "/html/body/div[1]/div/div[2]/div[1]/div[2]/form/div[2]/div/div[1]/div[3]/div[1]/div[1]/div/div/div[1]/div/div[1]/input",
        "value": "$PW"
      },
      {
        "type": "click",
        "element": "/html/body/div[1]/div/div[2]/div[1]/div[2]/form/div[2]/div/div[1]/div[3]/div[1]/div[3]/div/div/div[1]/div/div[1]/input"
      },
      {
        "type": "write",
        "element": "/html/body/div[1]/div/div[2]/div[1]/div[2]/form/div[2]/div/div[1]/div[3]/div[1]/div[3]/div/div/div[1]/div/div[1]/input",
        "value": "$PW"
      },
      {
        "type": "click",
        "element": "/html/body/div[1]/div/div[2]/div[1]/div[2]/form/div[2]/div/div[2]/div[1]/div/span/span"
      }
    ]
  },

  "modify": {
    "headless": false,
    "options": {
      "checkboxes": [],
      "radioboxes": [],
      "textfields": []
    },
    "actions": []
  },
  "delete": {
    "headless": false,
    "options": {
      "checkboxes": [],
      "radioboxes": [],
      "textfields": []
    },
    "actions": []
  }
}